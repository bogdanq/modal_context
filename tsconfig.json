{
  "compilerOptions": {
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noEmit": true,
    "target": "es5",
    "jsx": "preserve",
    "module": "commonjs",
    "declaration": true,
    "outDir": "./ts-built",
    "rootDir": "src"
  },
  "exclude": ["node_modules"],
  "include": ["src"]
}
